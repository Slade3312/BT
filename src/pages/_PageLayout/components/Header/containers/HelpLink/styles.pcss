@import "styles/constants.pcss";

.component {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100%;
  padding: 0 15px;
  color: #f2f2f2;
  font-size: 15px;
  font-family: var(--mainFontFamily);
  text-decoration: solid rgba(196, 196, 196, 0.2) 1px;

  &.active {
    background: rgba(196, 196, 196, 0.4);
  }
}
