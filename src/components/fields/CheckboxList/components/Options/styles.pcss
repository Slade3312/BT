@import "styles/constants.pcss";

.container {
  display: flex;
  flex-direction: column;
  align-items: flex-start;
}

.option {
  display: flex;
  align-items: center;
  cursor: pointer;
}

.content {
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 15px;
  font-family: var(--mainFontFamily);
  margin-left: 16px;
}
