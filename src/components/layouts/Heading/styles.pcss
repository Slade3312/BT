@import "styles/constants.pcss";

.component {
  padding: 0;
  margin: auto 0;
  color: inherit;
  font: 17px/1.4117 var(--mainFontFamily);
  font-weight: 400;
}

.h1 {
  font-size: 50px;
  line-height: 1.12;
}

.h2 {
  font-size: 41px;
  line-height: 1.1219;
}

.h3 {
  font-size: 30px;
  line-height: 1.3;
}

.h4 {
  font-size: 21px;
  line-height: 1.4285;
}

.h5 {
  font-size: 17px;
  line-height: 1.4117;
}

.bold {
  font-weight: 600;
}
