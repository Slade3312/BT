@import "styles/constants.pcss";

.container {
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    font-family: var(--mainFontFamily);
    z-index: 2;
    background: rgba(30, 30, 30, 0.68);
    position: absolute;
    height: 100%;
    width: 100%;
    top: 0;
    right: 0;
    border: 2px dashed #ffffff;
}

.dropFilesTitle {
    outline: none;
    color: #fff;
    font-size: 30px;
}